{
  "name": "hamlet-graphql",
  "version": "1.0.0",
  "description": "GraphQL Server for Multiple API's",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@types/bcrypt": "^3.0.0",
    "@types/graphql": "^14.0.5",
    "@types/jsonwebtoken": "^8.3.0",
    "apollo-server": "^2.3.2",
    "apollo-server-express": "^2.0.3",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dataloader": "^1.4.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "fs": "0.0.1-security",
    "graphql": "^0.13.2",
    "graphql-compose": "^4.8.1",
    "graphql-compose-json": "^1.1.2",
    "graphql-subscriptions": "^0.5.8",
    "http": "0.0.0",
    "js-schema": "^1.0.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mailgun.js": "^2.0.1",
    "moment": "^2.24.0",
    "node-fetch": "^2.2.0",
    "passport": "^0.4.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rethinkdb": "^2.3.3",
    "sawtooth-sdk": "^1.0.5",
    "sjcl": "^1.0.8",
    "subscriptions-transport-ws": "^0.9.14"
  },
  "main": "src/index.ts",
  "scripts": {
    "graphql": "babel-node --extensions \".ts\" src/index.ts",
    "start": "nodemon src/index.js --exec babel-node ",
    "serve": "babel-node dist/index.js",
    "build": "npm run build:proto_json && npm run build:types && npm run build:js",
    "build:proto_json": "node scripts/generate_proto_json.js > src/protos/generated_protos.json",
    "build:proto_static": "node scripts/generate_proto_static.js > src/protos/proto.js",
    "build:proto_types": "pbjs -t static-module protos/asset.proto protos/account.proto protos/holding.proto protos/offer_history.proto protos/offer.proto protos/payload.proto protos/property.proto protos/record.proto protos/rule.proto protos/proposal.proto | pbts -o src/protos/proto.d.ts -",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:db": "babel-node ./scripts/setup-db.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Colin McCullough",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.120",
    "@types/long": "^4.0.0",
    "@types/node": "^10.12.19",
    "@types/sjcl": "^1.0.28",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.9",
    "protobufjs": "^6.8.8",
    "standard": "^12.0.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "linebreak-style": 0,
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
